class Solution {
    public int wateringPlants(int[] plants, int capacity) {
       
        int amount=capa;
        int count=0;
        //missing for loop
            else if(amount<plants[i]){
                count+=i;
                amount=capa;
                count+=(i+1);
                amount-=plants[i];
            }
        }
        
        
        return count;
    }
}
